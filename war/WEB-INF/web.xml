    <filter>
    	<filter-name>ObjectifyFilter</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ObjectifyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter>
	    <filter-name>SessionFilter</filter-name>
	    <filter-class>si.stecce.societies.crowdtasking.session.SessionFilter</filter-class>
	    <init-param>
	        <param-name>unrestricted</param-name>
			<param-value>/welcome.html</param-value>
		</init-param>
	    <init-param>
	        <param-name>loginUrl</param-name>
			<param-value>/login</param-value>
		</init-param>
	    <init-param>
	        <param-name>logoutUrl</param-name>
			<param-value>/logout</param-value>
		</init-param>
		<init-param>
	        <param-name>registerUrl</param-name>
			<param-value>/register</param-value>
		</init-param>
	</filter>
	<filter-mapping>
	    <filter-name>SessionFilter</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
