	<servlet-mapping>
	   <servlet-name>jersey</servlet-name>
	   <url-pattern>/rest/*</url-pattern>
	</servlet-mapping>	 
    <servlet>
		<servlet-name>CSAdmin</servlet-name>
		<jsp-file>WEB-INF/jsp/spaces.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>CSAdmin</servlet-name>
        <url-pattern>/cs/admin</url-pattern>
	</servlet-mapping>

    <servlet>
        <servlet-name>SendMessageServlet</servlet-name>
        <servlet-class>
            si.stecce.societies.crowdtasking.gcm.SendMessageServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>SendMessageServlet</servlet-name>
        <url-pattern>/send</url-pattern>
    </servlet-mapping>

