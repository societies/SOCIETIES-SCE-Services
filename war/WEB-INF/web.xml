	<servlet>
		<servlet-name>login</servlet-name>
		<!--servlet-class>si.stecce.societies.crowdtasking.session.Login</servlet-class-->
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.session.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TaskController</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.controller.TaskController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TaskController</servlet-name>
		<url-pattern>/task/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TasksController</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.controller.TasksController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TasksController</servlet-name>
		<url-pattern>/tasks/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RootController</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.controller.RootController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RootController</servlet-name>
		<url-pattern>/profile</url-pattern>
		<url-pattern>/register</url-pattern>
		<url-pattern>/menu</url-pattern>
		<url-pattern>/settings</url-pattern>
		<url-pattern>/mobile</url-pattern>
		<url-pattern>/newsfeed</url-pattern>
		<url-pattern>/AndroidMenu</url-pattern> 
	</servlet-mapping>
	<servlet>
		<servlet-name>CommunityController</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.controller.CommunityController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CommunityController</servlet-name>
		<url-pattern>/community/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CSController</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.controller.CSController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CSController</servlet-name>
		<url-pattern>/cs/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PublicDisplayController</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.controller.PublicDisplayController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PublicDisplayController</servlet-name>
		<url-pattern>/publicDisplay/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Admin</servlet-name>
		<servlet-class>si.stecce.societies.crowdtasking.admin.Admin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Admin</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>menu</welcome-file>
	</welcome-file-list>
